{
  "name": "gb3-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local-gb2": "ng serve --configuration=development-local-gb2 --host 0.0.0.0 --port 4242 --poll 1000",
    "build-local": "ng build --configuration=development",
    "build-local-gb2": "ng build --configuration=development-local-gb2",
    "build-dev-ebp": "node ./update-version.js && ng build --configuration=production-dev-ebp",
    "build-uat": "ng build --configuration=production-uat",
    "build-staging": "ng build --configuration=production-staging",
    "build-staging-using-productive-gb2-backend": "ng build --configuration=production-staging-using-productive-gb2-backend",
    "build-production": "ng build --configuration=production",
    "watch": "ng build --configuration development",
    "test": "ng test",
    "test-ci": "ng test --no-watch --no-progress --code-coverage",
    "prepare": "node ./prepare.mjs",
    "update-version": "node ./update-version.js",
    "format-all": "prettier --write ./src",
    "generate-gb3-api": "npx swagger-typescript-api generate -p https://maps.zh.ch/api-docs/gb3/v4/swagger.yaml -o ./src/app/shared/models -n gb3-api-generated.interfaces.ts --no-client --extract-response-body --extract-request-body --extract-request-body && node ./remove-generated-api-version-references.js ./src/app/shared/models/gb3-api-generated.interfaces.ts",
    "lint": "eslint"
  },
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "dependencies": {
    "@angular/animations": "^20.0.5",
    "@angular/build": "^20.0.4",
    "@angular/cdk": "^20.0.4",
    "@angular/cli": "^20.0.4",
    "@angular/common": "^20.0.5",
    "@angular/compiler": "^20.0.5",
    "@angular/compiler-cli": "^20.0.5",
    "@angular/core": "^20.0.5",
    "@angular/forms": "^20.0.5",
    "@angular/localize": "^20.0.5",
    "@angular/material": "^20.0.4",
    "@angular/platform-browser": "^20.0.5",
    "@angular/platform-browser-dynamic": "^20.0.5",
    "@angular/router": "^20.0.5",
    "@arcgis/core": "4.33.10",
    "@gisktzh/cim-symbol-to-svg": "github:gisktzh/cim-symbol-to-svg",
    "@ngrx/effects": "^20.0.0",
    "@ngrx/entity": "^20.0.0",
    "@ngrx/operators": "^20.0.0",
    "@ngrx/router-store": "^20.0.0",
    "@ngrx/store": "^20.0.0",
    "@ngrx/store-devtools": "^20.0.0",
    "@terraformer/arcgis": "^2.1.2",
    "@types/file-saver": "^2.0.7",
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22",
    "@types/terraformer__arcgis": "^2.0.2",
    "@types/twitter-for-web": "^0.0.6",
    "angular-oauth2-oidc": "^20.0.0",
    "angular-resizable-element": "^8.0.0",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.7",
    "file-saver": "^2.0.5",
    "immer": "^11.0.0",
    "material-icons": "^1.13.12",
    "ng2-charts": "^8.0.0",
    "ngx-ui-tour-md-menu": "^15.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@eslint/js": "^9.19.0",
    "@ngrx/eslint-plugin": "^20.0.0",
    "@ngrx/schematics": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "^20.1.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-rxjs-angular-x": "^0.1.0",
    "eslint-plugin-rxjs-x": "~0.8.0",
    "husky": "^9.0.0",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.3.3",
    "puppeteer": "^24.0.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.20.0"
  }
}
