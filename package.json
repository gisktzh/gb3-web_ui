{
  "name": "gb3-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local-gb2": "ng serve --configuration=development-local-gb2",
    "build-local": "ng build --configuration=development",
    "build-local-gb2": "ng build --configuration=development-local-gb2",
    "build-dev-ebp": "node ./update-version.js && ng build --configuration=production-dev-ebp",
    "build-uat": "ng build --configuration=production-uat",
    "build-staging": "ng build --configuration=production-staging",
    "build-staging-using-productive-gb2-backend": "ng build --configuration=production-staging-using-productive-gb2-backend",
    "build-production": "ng build --configuration=production",
    "watch": "ng build --configuration development",
    "test": "ng test",
    "test-ci": "ng test --no-watch --no-progress --code-coverage",
    "prepare": "node ./prepare.mjs",
    "update-version": "node ./update-version.js",
    "format-all": "prettier --write ./src",
    "generate-gb3-api": "npx swagger-typescript-api -p https://maps.zh.ch/api-docs/gb3/v3/swagger.yaml -o ./src/app/shared/models -n gb3-api-generated.interfaces.ts --no-client --extract-response-body --extract-request-body --extract-request-body && node ./remove-generated-api-version-references.js ./src/app/shared/models/gb3-api-generated.interfaces.ts"
  },
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/localize": "^18.1.0",
    "@angular/material": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@arcgis/core": "~4.30.0",
    "@ngrx/effects": "^18.0.0",
    "@ngrx/entity": "^18.0.0",
    "@ngrx/operators": "^18.0.1",
    "@ngrx/router-store": "^18.0.0",
    "@ngrx/store": "^18.0.0",
    "@ngrx/store-devtools": "^18.0.0",
    "@terraformer/arcgis": "^2.1.2",
    "@types/file-saver": "^2.0.7",
    "@types/geojson": "^7946.0.10",
    "@types/node": "^20.8",
    "@types/terraformer__arcgis": "^2.0.2",
    "@types/twitter-for-web": "^0.0.6",
    "@types/uuid": "^10.0.0",
    "@uppy/core": "^3.12.0",
    "@uppy/drop-target": "^2.1.0",
    "@uppy/file-input": "^3.1.2",
    "angular-oauth2-oidc": "^17.0.0",
    "angular-resizable-element": "^7.0.2",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.7",
    "file-saver": "^2.0.5",
    "immer": "^10.0.1",
    "material-icons": "^1.13.12",
    "ng2-charts": "^6.0.0",
    "ngx-ui-tour-md-menu": "^13.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@ngrx/eslint-plugin": "^17.1.1",
    "@ngrx/schematics": "^18.0.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "eslint-plugin-rxjs-angular": "^2.0.0",
    "husky": "^9.0.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.3.3",
    "puppeteer": "^23.0.0",
    "typescript": "~5.5.0"
  }
}
